mixin nosotros_text(image, text, imagetwo, texttwo)
  article.nosotros
    .nosotros_img.move.u
      i.nosotros_imge(style=`background-image: url(${image});`)
    .nosotros_text.move
      p.texto.nosotros_text_p= text
        br
        br
        br
        a(href="").btn Contactanos
    .nosotros_texttwo.move
      p.texto.nosotros_text_p= texttwo
        br
        br
        br
        a(href="").btn Contactanos
    .nosotros_imgtwo.move.d
      i.nosotros_imge(style=`background-image: url(${imagetwo});`)
