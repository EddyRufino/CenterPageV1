mixin calidad_servicio(moderno1, moderno2, calidad1, calidad2, atencion1, atencion2)
  article.calidad-servicio
    div.calidad-servicio-container
      div.calidad-servicio-container-moderno
        .calidad-servicio-moderno-img.all-img.img-moderno-servicio
        h3.calidad-servicio-moderno-title.all-title-servicio= moderno1
        span.calidad-servicio-moderno-description.all-decrip-servicio= moderno2
      div.calidad-servicio-container-calidad
        .calidad-servicio-calidad-img.all-img.img-calidad-servicio
        h3.calidad-servicio-calidad-title.all-title-servicio= calidad1
        span.calidad-servicio-calidad-description.all-decrip-servicio= calidad2
        .img-two
      div.calidad-servicio-container-atencion
        .calidad-servicio-atencion-img.all-img.img-atencion-servicio
        h3.calidad-servicio-atencion-title.all-title-servicio= atencion1
        span.calidad-servicio-atencion-description.all-decrip-servicio= atencion2
